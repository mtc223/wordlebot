extends layout



block content
  .navbar
    .title Wordlebot
  
  each row in ['0','1','2','3','4','5']
    div(class="row",id="r"+row)
      each btn in ['0','1','2','3','4']
        div(class="column1",id="d"+btn)
          .ui.black.button(id="l"+btn+row)
            | 

  div(class="row",id="button")
    div(class="column1",id="ebtn")
      .ui.green.button#enter
        |Start

