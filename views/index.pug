extends layout



block content
  .navbar
    .title Wordlebot
  
  div(class="row",id="buttons")
    each btn in ['1','2','3','4','5']
      div(class="column1",id="d"+btn)
        .ui.black.button(id="l"+btn)
          |
  div(class="row",id="button")
    div(class="column1",id="ebtn")
      .ui.green.button#enter
        |   Start
        |

  each row in ['1','2','3','4','5','6']
    div(class="row",id="m"+row)
      p(id="r"+row)